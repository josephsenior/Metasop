# Database Configuration
DATABASE_URL="postgresql://user:password@localhost:5432/multiagent?schema=public"
DIRECT_URL="postgresql://user:password@localhost:5432/multiagent?schema=public"

# AI/LLM Configuration
# Use one of: GOOGLE_AI_API_KEY, GEMINI_API_KEY, or METASOP_LLM_API_KEY
GOOGLE_AI_API_KEY="your-google-ai-api-key"
METASOP_LLM_PROVIDER="gemini" # Options: gemini, mock
METASOP_LLM_MODEL="gemini-3-flash-preview"

# Authentication
JWT_SECRET="your-super-secret-key-change-in-production"
JWT_EXPIRES_IN="7d"

# MetaSOP Agent Settings (Optional)
# METASOP_AGENT_TIMEOUT=300000
# METASOP_AGENT_RETRIES=2

# Environment
NODE_ENV="production"
